{
  "expo": {
    "name": "wanderlist",
    "slug": "wanderlist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Wanderlist.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ankitapanjwani.wanderlist",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCWDApxY413T92k40jFXYWmQAd91FiMPq4"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {}
    }
  }
}
